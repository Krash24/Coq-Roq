Section CoqLab01.
Context (A B C D : Prop).

(*           H1           H2           H3 *)
Lemma q1 : (A -> B) -> (B -> C) -> (A -> C).
Proof.
 intros  H1 H2 H3. 
 apply H2. 
 apply H1. 
 apply H3. 
Qed.
 
(* Anything implies itself. *)
Lemma q2 : A -> A.
Proof.
  intros.
  apply H.
Qed.

(* Extra hypotheses can be introduced without affecting
 * provability. *)
Lemma q3 : A -> (B -> A).
Proof.
  intros. 
  apply H.
Qed.

(* The order of the hypotheses is irrelevant. *)
Lemma q4 : (A -> B -> C) -> B -> A -> C.
Proof.
  intros.
  apply H.
  assumption. 
  apply H0. 
Qed.
  
(* Duplicate hypotheses can be merged. *)
Lemma q5 : (A -> A -> B) -> A -> B.
Proof.
  intros.
  apply H. 
  apply H0. 
  assumption. 
Qed. 

(* Duplicate hypotheses can be introduced. *)
Lemma q6 : (A -> B) -> A -> A -> B.
Proof.
  intros.
  apply H. 
  apply H0.
Qed. 

Lemma q7 : (A -> B) -> (A -> C) -> (B -> C -> D) -> A -> D.
Proof.
  intros. 
  apply H1. 
  apply H. 
  apply H2. 
  apply H0.
  assumption. 
Qed.

Lemma q8 : ((((A -> B) -> A) -> A) -> B) -> B.
Proof.
  intros. 
  apply H. 
  intros H0. 
  apply H0. 
  intros H1. 
  apply H.  
  intros H2.
  exact H1.
Qed.



End CoqLab01.